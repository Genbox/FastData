    
    public static bool Contains(Int32 value)
    {
        if (value < 1 || value > 10)
           return false;

        switch (unchecked((uint)value))
        {
            case 1:
                 return value.Equals(1);
            case 2:
                 return value.Equals(2);
            case 3:
                 return value.Equals(3);
            case 4:
                 return value.Equals(4);
            case 5:
                 return value.Equals(5);
            case 6:
                 return value.Equals(6);
            case 7:
                 return value.Equals(7);
            case 8:
                 return value.Equals(8);
            case 9:
                 return value.Equals(9);
            case 10:
                 return value.Equals(10);
        }
        return false;
    }