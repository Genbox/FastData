#include <string>
#include <array>
#include <cstdint>

class EytzingerSearch_Int32_1
{
    inline static const std::array<int32_t, 1> entries = {
        1
    };

public:
    static bool contains(const int32_t& value)
    {
       if (const size_t len = value.length(); len != 1)
           return false;

        unsigned int i = 0;
        while (i < entries.size())
        {
            const int comparison = entries[i].compare(value);

            if (comparison == 0)
                return true;

            if (comparison < 0)
                i = 2 * i + 2;
            else
                i = 2 * i + 1;
        }

        return false;
    }

    static constexpr int item_count = 1;
};