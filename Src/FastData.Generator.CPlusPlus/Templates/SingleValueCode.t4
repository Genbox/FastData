<#= Model.GetMethodHeader(MethodType.TryLookup) #>

        if (<#= Model.GetEqualFunction(Common.LookupKeyName, itemLabel) #>) {
            value = <#= Common.IsPrimitive ? "&" : "" #>stored_value;
